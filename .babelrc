{
  "presets": [
    "es2015",
    ["env", {"modules": false}],
    "stage-2"
  ],
  "plugins": [
    ["transform-runtime", {"polyfill": true}],
    "syntax-dynamic-import",
    "dynamic-import-node",
    "transform-runtime"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
