{
  "name": "nb-vue-ssr",
  "version": "1.0.0",
  "description": "NewBanker ssr using vue and koa",
  "main": "index.js",
  "scripts": {
    "slink": "cd node_modules && ln -sf ../server @s",
    "dev": "npm run slink && NODE_ENV=development DEBUG=request nodemon server/index.js --watch server",
    "start": "NODE_ENV=production pm2 start server/index.js -i 4 --name app",
    "reload": "pm2 reload app",
    "delete": "pm2 delete app",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^2.3.0",
    "detect-port": "^1.2.3",
    "element-ui": "^2.4.2",
    "es6-promise": "^4.2.2",
    "koa": "^2.3.0",
    "koa-compress": "^2.0.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^3.0.1",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "lru-cache": "^4.1.1",
    "nb-koa-proxy": "^1.0.0",
    "nb-random-port": "^1.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.13",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "config": "^1.30.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "semver": "^5.4.1",
    "sw-precache-webpack-plugin": "^0.11.4",
